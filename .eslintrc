{
   "extends": "eslint:recommended",
   "rules": { //设置校验的规则
      "semi": ["error", "always"],
      "quotes": ["error", "single"],
      "no-console":"off"
   },
   "parserOptions": {
      "ecmaVersion": 6,
      "sourceType": "module",
   },
   "env": {
      "browser": true,
      "node": true,
      "amd":true
   },
   "globals": {
      "requirejs":"readonly"
   }
}